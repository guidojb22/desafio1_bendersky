<div>
    <h1>Desafio 5</h1>

    <h3>Lista de prodcutos:</h3>

    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>title</th>
                <th>description</th>
                <th>code</th>
                <th>price</th>
                <th>status</th>
                <th>stock</th>
                <th>category</th>
                <th>thumbnails</th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.code}}</td>
                <td>{{this.price}}</td>
                <td>
                    {{#if this.status}}
                    Activo
                    {{else}}
                    Desactivado
                    {{/if}}
                </td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.thumbnails}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>